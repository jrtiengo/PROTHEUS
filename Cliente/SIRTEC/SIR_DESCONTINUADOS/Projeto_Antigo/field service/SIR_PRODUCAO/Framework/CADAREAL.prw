Static Function ViewDef()

Local oModel   := FWLoadModel("CADAREAL")
Local oStruZZQ := FWFormStruct(2,"ZZQ") 
Local oView	   := FWFormView():New()

oView:SetModel(oModel)
oView:AddField("VIEW_ZZQ", oStruZZQ, "ZZQUNICO")
