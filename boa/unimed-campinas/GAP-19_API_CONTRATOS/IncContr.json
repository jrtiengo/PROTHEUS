{
	"title": "Inclusao de Contratos",
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"required": [
		"cnpj",
		"ncontrato",
		"idint"
	],
	"properties": {
		"cnpj": {
			"description": "CNPJ da empresa destino",
			"type": "string",
			"minLength": 14,
			"maxLength": 14
		},
		"idint": {
			"description": "id de integração do fluig",
			"type": "string",
			"minLength": 10,
			"maxLength": 10
		},
		"ncontrato": {
			"description": "0000000000000015",
			"type": "string",
			"minLength": 15,
			"maxLength": 15
		},
		"contrato": {
			"id": "contrato",
			"description": "Contratos",
			"type": "array",
			"minItems": 1,
			"items": {
				"type": "object",
				"required": [
					"CN9_TPCTO",
					"CN9_DTINC",
					"CN9_UNVIGE",
					"CN9_VIGE",
					"CN9_MOEDA",
					"CN9_CONDPG",
					"CN9_FLGREJ",
					"CN9_INDICE",
					"CN9_FLGCAU",
					"CN9_ALTCLA",
					"CN9_VLDCTR",
					"CN9_APROV",
					"CN9_GRPAPR",
					"CN9_NATURE",
					"CN9_DEPART",
					"CN9_PERI",
					"CN9_UNPERI",
					"CN9_MODORJ",
					"CN9_PRORAT"
				],
				"properties": {
					"CN9_TPCTO": {
						"description": "infomar o tipo do contrato",
						"type": "string",
						"minLength": 3,
						"maxLength": 3
					},
					"CN9_DTINC": {
						"description": "informar a data de inicio do contrato",
						"type": "string",
						"pattern": "(0[1-9]|[12][0-9]|3[01])\\/(0[1-9]|1[1,2])\\/(19|20)\\d{2}"
					},
					"CN9_XDTORI": {
						"description": "informar a data original de inicio do contrato, conforme minuta, para contratos Legado",
						"type": "string",
						"pattern": "(0[1-9]|[12][0-9]|3[01])\\/(0[1-9]|1[1,2])\\/(19|20)\\d{2}"
					},
					"CN9_XCLICO": {
						"description": "informar o cliente confederacao",
						"type": "string",
						"minLength": 0,
						"maxLength": 6
					},
					"CN9_XLOJCO": {
						"description": "informar a loja confederacao",
						"type": "string",
						"minLength": 0,
						"maxLength": 2
					},
					"CN9_UNVIGE": {
						"description": "informar a unidade de vigência do contrato",
						"type": "string",
						"minLength": 1,
						"maxLength": 1
					},
					"CN9_VIGE": {
						"description": "informar a quantidade da vigência, conforme o tipo",
						"type": "integer",
						"minimum": 1,
						"maximum": 999999
					},
					"CN9_MOEDA": {
						"description": "informar a moeda do contrato",
						"type": "integer",
						"minimum": 1,
						"maximum": 99
					},
					"CN9_CONDPG": {
						"description": "informar a condição de pagamento do contrato",
						"type": "string",
						"minLength": 3,
						"maxLength": 3
					},
					"CN9_FLGREJ": {
						"description": "contrato pode ser reajustado?",
						"type": "string",
						"minLength": 1,
						"maxLength": 1
					},
					"CN9_INDICE": {
						"description": "se reajuste igual a sim, informar o indice a ser aplicado",
						"type": "string",
						"minLength": 3,
						"maxLength": 3
					},
					"CN9_FLGCAU": {
						"description": "contrato tem caucao",
						"type": "string",
						"minLength": 1,
						"maxLength": 1
					},
					"CN9_OBJCTO": {
						"description": "informar o texto referente ao objeto do contrato",
						"type": "string",
						"minLength": 0,
						"maxLength": 300
					},
					"CN9_ALTCLA": {
						"description": "informar o texto referente a clausula do contrato",
						"type": "string",
						"minLength": 0,
						"maxLength": 300
					},
					"CN9_VLDCTR": {
						"description": "valida acesso ao contrato",
						"type": "string",
						"minLength": 1,
						"maxLength": 1
					},
					"CN9_APROV": {
						"description": "grupo de aprovação para controle de alçada do contrato",
						"type": "string",
						"minLength": 0,
						"maxLength": 6
					},
					"CN9_GRPAPR": {
						"description": "grupo de aprovação para controle de alcada das medicoes",
						"type": "string",
						"minLength": 0,
						"maxLength": 6
					},
					"CN9_NATURE": {
						"description": "natureza do contrato",
						"type": "string",
						"minLength": 10,
						"maxLength": 10
					},
					"CN9_DEPART": {
						"description": "Area do contrato - para controle de acesso e envio de WF",
						"type": "string",
						"minLength": 6,
						"maxLength": 6
					},
					"CN9_PERI": {
						"description": "periodiciodade do reajuste",
						"type": "integer",
						"minimum": 1,
						"maximum": 999999
					},
					"CN9_UNPERI": {
						"description": "unidade de periodicidade do reajuste",
						"type": "string",
						"minLength": 1,
						"maxLength": 1
					},
					"CN9_MODORJ": {
						"description": "unidade de periodicidade do reajuste",
						"type": "string",
						"minLength": 1,
						"maxLength": 1
					},
					"CN9_PRORAT": {
						"description": "unidade de periodicidade do reajuste",
						"type": "string",
						"minLength": 1,
						"maxLength": 1
					},
					"Fornecedor": {
						"description": "Fornecedor",
						"type": "array",
						"minItems": 1,
						"items": {
							"type": "object",
							"required": [
								"CNC_CODIGO",
								"CNC_LOJA"
							],
							"properties": {
								"CNC_CODIGO": {
									"description": "Codigo Fornecedor",
									"type": "string",
									"minLength": 1,
									"maxLength": 6
								},
								"CNC_LOJA": {
									"description": "Loja Fornecedor",
									"type": "string",
									"minLength": 1,
									"maxLength": 2
								}
							}
						}
					},
					"Planilha": {
						"id": "Planilha",
						"description": "Planilha",
						"type": "array",
						"minItems": 1,
						"items": {
							"type": "object",
							"required": [
								"CNA_TIPPLA",
								"CNA_VLTOT"
							],
							"CNA_TIPPLA": {
								"description": "tipo da planilha do contrato",
								"type": "string",
								"minLength": 3,
								"maxLength": 3
							},
							"CNA_VLTOT": {
								"description": "Valor Total",
								"type": "integer",
								"minLength": 2,
								"maxLength": 14
							}
						}
					},
					"Acessos": {
						"type": "array",
						"description": "Acessos ao contrato",
						"minItems": 1,
						"items": {
							"type": "object",
							"required": [
								"CNN_USRCOD"
							],
							"properties": {
								"CNN_USRCOD": {
									"description": "Código do Usuário",
									"type": "string",
									"minLength": 6,
									"maxLength": 6
								},
								"CNN_GRPCOD": {
									"description": "Código do Grupo",
									"type": "string",
									"minLength": 6,
									"maxLength": 6
								},
								"CNN_TRACOD": {
									"description": "Código da Transação",
									"type": "string",
									"minLength": 3,
									"maxLength": 3
								}
							}
						}
					},
					"Filiais": {
						"type": "array",
						"description": "Filiais de acessos",
						"minItems": 1,
						"items": [
							{
								"description": "Filiais de acesso",
								"type": "string",
								"minLength": 2,
								"maxLength": 2
							}
						]
					},
					"itens da planilha": {
						"type": "array",
						"description": "Itens da planilha",
						"minItems": 1,
						"items": [
							{
								"type": "object",
								"properties": {
									"CNB_PRODUT": {
										"description": "Produto",
										"type": "string",
										"minLength": 12,
										"maxLength": 12
									},
									"CNB_QUANT": {
										"description": "Quantidade",
										"type": "integer",
										"minLength": 2,
										"maxLength": 12
									},
									"CNB_VLUNIT": {
										"description": "Valor Unitario",
										"type": "integer",
										"minLength": 2,
										"maxLength": 14
									},
									"CNB_CONTA": {
										"description": "Conta Contabil",
										"type": "string",
										"minLength": 20,
										"maxLength": 20
									},
									"CNB_TE": {
										"description": "Tipo Entrada",
										"type": "string",
										"minLength": 3,
										"maxLength": 3
									},
									"CNB_CC": {
										"description": "Centro Custo",
										"type": "string",
										"minLength": 9,
										"maxLength": 9
									}
								},
								"required": [
									"CNB_PRODUT",
									"CNB_QUANT",
									"CNB_VLUNIT",
									"CNB_CONTA",
									"CNB_TE",
									"CNB_CC"
								]
							}
						]
					},
					"Cronograma": {
						"type": "array",
						"description": "Cronograma Financeiro",
						"minItems": 1,
						"items": {
							"type": "object",
							"properties": {
								"CRG_NPERIO": {
									"description": "Numero Periodo",
									"type": "integer",
									"minLength": 1,
									"maxLength": 1
								},
								"CRG_NDIAS": {
									"description": "Numero de dias para as parcelas",
									"type": "integer",
									"minLength": 2,
									"maxLength": 2
								},
								"CRG_NULTDI": {
									"description": "Ultimo dia do medicoes",
									"type": "integer",
									"minLength": 1,
									"maxLength": 1
								},
								"CRG_CDTCOM": {
									"description": "Competencia do inicio do Programa",
									"type": "string",
									"minLength": 7,
									"maxLength": 7
								},
								"CRG_DTPREV": {
									"description": "DT prevista para primeira parcela",
									"type": "string",
									"minLength": 1,
									"maxLength": 1
								},
								"CRG_NPARC": {
									"description": "Numero das Parcelas",
									"type": "string",
									"minLength": 3,
									"maxLength": 3
								},
								"CRG_CCOND": {
									"description": "Condicao de Pagto",
									"type": "string",
									"minLength": 3,
									"maxLength": 3
								},
								"CRG_NTXJUR": {
									"description": "Taxa de Juros",
									"type": "integer",
									"minLength": 4,
									"maxLength": 3
								}
							}
						}
					},
					"documentos": {
						"type": "array",
						"description": "documentos contrato",
						"minItems": 1,
						"items": {
							"items": {
								"type": "object",
								"properties": {
									"CNK_TPDOC": {
										"description": "Taxa de Juros",
										"type": "integer",
										"minLength": 4,
										"maxLength": 3
									},
									"CNK_DESCRI": {
										"description": "Taxa de Juros",
										"type": "integer",
										"minLength": 4,
										"maxLength": 3
									},
									"CNK_DTEMIS": {
										"description": "Data de Validade",
										"type": "integer",
										"pattern": "(0[1-9]|[12][0-9]|3[01])\\/(0[1-9]|1[1,2])\\/(19|20)\\d{2}"
									},
									"CNK_DTVALI": {
										"description": "Data de Validade",
										"type": "integer",
										"pattern": "(0[1-9]|[12][0-9]|3[01])\\/(0[1-9]|1[1,2])\\/(19|20)\\d{2}"
									},
									"CNK_OBS": {
										"description": "Observacao",
										"type": "String",
										"minLength": 0,
										"maxLength": 30
									},
									"ANEXOS": {
										"description": "Anexo em Base64",
										"type": "string",
										"minLength": 0,
										"maxLength": 999
									}
								},
								"required": [
								]
							}
						}
					}
				}
			}
		}
	}
}