@ LI,100 PSAY STR0044 + cDgss
@ LI,131 PSAY "|"

LI ++
@ LI,000 PSAY "| "+STR0018 +  Transform(DESC_CGC,PesqPict("SA1","A1_CGC"))//" R.U.C: "
@ LI,100 PSAY STR0019 + cBPS 					//" B.P.S.: "
@ LI,131 PSAY "|"

