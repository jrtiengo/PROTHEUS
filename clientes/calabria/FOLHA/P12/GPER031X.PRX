Local cDescr
If mv_par03 == 1
	cDescr := STR0045
ElseIf mv_par03 ==2
	cDescr := STR0046
ElseIf mv_par03 == 3
	cDescr := STR0047
ElseIf mv_par03 == 4
	cDescr := STR0048
ElseIf mv_par03 == 3
	cDescr := STR0049
Endif

LI ++
@ LI,00 PSAY "*"+REPLICATE("=",130)+"*"

LI ++
