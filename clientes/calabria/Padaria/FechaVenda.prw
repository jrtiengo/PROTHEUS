Static Function Fechar()

Local cQuery
Local _flag := .F.

cQuery :=" SELECT * FROM "+ RetSqlName('SZP') +" "
cQuery +=" WHERE ZP_FILIAL = '"+ xFilial('SZP') +"' "
cQuery +=" AND ZP_EMISSAO BETWEEN '"+ dtos(MV_PAR01)+"' AND '"+ dtos(MV_PAR02)+"' " 
cQuery +=" AND ZP_PAGO = '2' " 
cQuery +=" AND D_E_L_E_T_ = '' "
cQuery +=" ORDER BY ZP_NUM,ZP_EMISSAO "


If Select('TMPSZP') <> 0 
	TMPSZP->(DbCloseArea())
EndIf


TcQuery cQuery New Alias 'TMPSZP'

DbSelectArea('TMPSZP')
TMPSZP->(DbGoTop())
ProcRegua( TMPSZP->(RecCount()) )
Do While TMPSZP->(!Eof()) 
	IncProc("Processando...Cracha: "+Alltrim(TMPSZP->ZP_CODCRA))
    
    DbSelectArea('SZP')
    DbSetOrder(2)
    DbSeek(xFilial('SZP')+TMPSZP->ZP_NUM+TMPSZP->ZP_EMISSAO)             
	
	SZP->(RecLock('SZP',.F.))
	   
		SZP->ZP_PAGO = '1'
		_flag := .T.
	SZP->(MsUnLock())

TMPSZP->(DbSkip())
EndDo

TMPSZP->(DbCloseArea())	



Return(_flag)     



